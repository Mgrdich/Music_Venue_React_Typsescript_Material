{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@petejkim/react-scripts-ts-sass": "^2.15.2",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.0.1",
    "react-scripts-ts": "3.1.0"
  },
  "scripts": {
    "build:css": "node-sass src/Style/style.scss -o",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
    "start": "npm-run-all -p watch:css start:js",
    "start:js": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "typescript": "^3.6.3"
  }
}
